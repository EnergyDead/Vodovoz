  <DataSets>
    <DataSet Name="Data">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT
	CASE cars.type_of_use
		WHEN 'CompanyLargus' THEN 'Ларгусы'
		WHEN 'CompanyGAZelle' THEN 'ГАЗели'
		ELSE 'Раскат'
	END AS type_of_use,
	cars.is_raskat,
	route_lists.id,
	route_lists.driver_id,
	CASE route_lists.status
		WHEN 'New' THEN 'Новый'
        WHEN 'Confirmed' THEN 'Подтверждён'
		WHEN 'InLoading' THEN 'На погрузке'
		WHEN 'EnRoute' THEN 'В пути'
		WHEN 'OnClosing' THEN 'Сдаётся'
		WHEN 'NotDelivered' THEN 'Не сдан'
		WHEN 'MileageCheck' THEN 'Проверка километража'
		WHEN 'Closed' THEN 'Закрыт'
	END AS status,
	reg_number,
	CONCAT_WS(' ',
			employees.last_name,
			IF(employees.name = '',
				NULL,
				employees.name),
			IF(employees.patronymic = '',
				NULL,
				employees.patronymic)) AS driver_name,
                
	(SELECT 
		SUM(IF(ods.order_status NOT IN ('NotDelivered','DeliveryCanceled'), items.price
		* IFNULL(items.actual_count, items.count) - items.discount_money, 0))
		FROM
			order_items as items
		LEFT JOIN
			orders as ods ON items.order_id = ods.id
		WHERE EXISTS
			(SELECT * FROM route_lists rl
			LEFT JOIN route_list_addresses as rla ON rla.route_list_id = rl.id
			LEFT JOIN cars as crs ON rl.car_id = crs.id
			WHERE
				(rl.id = route_lists.id)
                AND
				(rl.date BETWEEN @start_date AND @end_date)
				AND rla.order_id = ods.id
				AND ((crs.type_of_use = 'CompanyLargus' OR crs.type_of_use = 'CompanyGAZelle') OR crs.is_raskat = 1)
		)) AS route_list_sum,
				
	COUNT(route_list_addresses.id) AS address_count,
	(SELECT 
			COUNT(route_list_addresses.id)
		FROM
			route_list_addresses
		WHERE
			route_list_addresses.route_list_id = route_lists.id
				AND (route_list_addresses.status = 'Canceled'
				OR route_list_addresses.status = 'Transfered')) AS not_delivered,
	IFNULL(SUM((SELECT 
					SUM(order_items.count)
				FROM
					order_items
						LEFT JOIN
					nomenclature ON nomenclature.id = order_items.nomenclature_id
				WHERE
					order_items.order_id = route_list_addresses.order_id
						AND nomenclature.category = 'water' AND nomenclature.tare_volume = 'Vol19L')),
			0) AS water19_amount,
	IFNULL(SUM((SELECT 
					SUM(order_items.count)
				FROM
					order_items
						LEFT JOIN
					nomenclature ON nomenclature.id = order_items.nomenclature_id
				WHERE
					order_items.order_id = route_list_addresses.order_id
						AND nomenclature.category = 'water' AND nomenclature.tare_volume = 'Vol6L')
					),
			0) AS water6_amount,
	SUM(IF((SELECT 
				SUM(order_items.count)
			FROM
				order_items
					LEFT JOIN
				nomenclature ON nomenclature.id = order_items.nomenclature_id
			WHERE
				order_items.order_id = route_list_addresses.order_id
					AND nomenclature.category = 'water' AND nomenclature.tare_volume = 'Vol19L') IS NULL,
		2,
		0)) AS water19_absent,
	IFNULL((SELECT 
					wages_movement_operations.money
				FROM
					wages_movement_operations
				WHERE
					route_lists.driver_wages_movement_operations_id = wages_movement_operations.id
						AND (route_lists.status = 'OnClosing'
						OR route_lists.status = 'MileageCheck'
						OR route_lists.status = 'Closed')),
			0) AS wages
FROM
	route_lists
		LEFT JOIN
	employees ON employees.id = route_lists.driver_id
		LEFT JOIN
	route_list_addresses ON route_list_addresses.route_list_id = route_lists.id
		LEFT JOIN
	orders ON route_list_addresses.order_id = orders.id
		LEFT JOIN
	delivery_points ON orders.delivery_point_id = delivery_points.id
		LEFT JOIN
	cars ON route_lists.car_id = cars.id
WHERE (route_lists.date BETWEEN @start_date AND @end_date)
      		AND ((cars.type_of_use = 'CompanyLargus' OR cars.type_of_use = 'CompanyGAZelle') OR cars.is_raskat = 1)
GROUP BY route_lists.id
ORDER BY driver_name</CommandText>
        <QueryParameters>
          <QueryParameter Name="start_date">
            <Value>=Parameters!start_date</Value>
          </QueryParameter>
          <QueryParameter Name="end_date">
            <Value>=Parameters!end_date</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
