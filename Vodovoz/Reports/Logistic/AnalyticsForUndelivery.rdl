  <DataSets>
    <DataSet Name="Data">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>#SET @start_date = '2020-08-20';
#SET @end_date = '2020-08-20 23:59';

SELECT
   T.week_begin,
   T.week_end,
   CAST(SUM(T.bottles_monday) as SIGNED) as bottles_monday,
   (COUNT(T.address_monday) * (SUM(T.address_monday) - SUM(T.address_monday) + 1) ) as address_monday,

   CAST(SUM(T.bottles_tuesday) as SIGNED) as bottles_tuesday,
   (COUNT(T.address_tuesday) * (SUM(T.address_tuesday) - SUM(T.address_tuesday) + 1) ) as address_tuesday,

   CAST(SUM(T.bottles_wednesday) as SIGNED) as bottles_wednesday,
   (COUNT(T.address_wednesday) * (SUM(T.address_wednesday) - SUM(T.address_wednesday) + 1) ) as address_wednesday,

   CAST(SUM(T.bottles_thursday) as SIGNED) as bottles_thursday,
   (COUNT(T.address_thursday) * (SUM(T.address_thursday) - SUM(T.address_thursday) + 1) ) as address_thursday,

   CAST(SUM(T.bottles_friday) as SIGNED) as bottles_friday,
   (COUNT(T.address_friday) * (SUM(T.address_friday) - SUM(T.address_friday) + 1) ) as address_friday,

   CAST(SUM(T.bottles_saturday) as SIGNED) as bottles_saturday,
   (COUNT(T.address_saturday) * (SUM(T.address_saturday) - SUM(T.address_saturday) + 1) ) as address_saturday,

   CAST(SUM(T.bottles_sunday) as SIGNED) as bottles_sunday,
   (COUNT(T.address_sunday) * (SUM(T.address_sunday) - SUM(T.address_sunday) + 1) ) as address_sunday

FROM
    (SELECT
        week_days.week_begin,
        week_days.week_end,

        IF(dayofweek = 0, order_sum.count, null)  AS bottles_monday,
        IF(dayofweek = 0, compiled_address, null)  AS address_monday,

        IF(dayofweek = 1 , order_sum.count, null)  AS bottles_tuesday,
        IF(dayofweek = 1 , compiled_address, null)  AS address_tuesday,

        IF(dayofweek = 2 , order_sum.count, null)  AS bottles_wednesday,
        IF(dayofweek = 2 , compiled_address, null)  AS address_wednesday,

        IF(dayofweek = 3 , order_sum.count, null)  AS bottles_thursday,
        IF(dayofweek = 3 , compiled_address, null)  AS address_thursday,

        IF(dayofweek = 4 , order_sum.count, null)  AS bottles_friday,
        IF(dayofweek = 4 , compiled_address, null)  AS address_friday,

        IF(dayofweek = 5 , order_sum.count, null)  AS bottles_saturday,
        IF(dayofweek = 5 , compiled_address, null)  AS address_saturday,

        IF(dayofweek = 6 , order_sum.count, null)  AS bottles_sunday,
        IF(dayofweek = 6 , compiled_address, null)  AS address_sunday
    FROM
        orders
            LEFT JOIN
        (
            SELECT
                orders.id as order_id,
                date_sub(delivery_date, interval (weekday(delivery_date)) day) as week_begin,
                date_add(delivery_date, interval (6-weekday(delivery_date)) day) as week_end,
                WEEKDAY(delivery_date) as dayofweek
            FROM
                orders
                    LEFT JOIN
                delivery_schedule ON delivery_schedule.id = orders.delivery_schedule_id
        ) as week_days ON week_days.order_id = orders.id
            LEFT JOIN
        (
            SELECT
                order_items.order_id,
                SUM(order_items.count) AS count
            FROM
                order_items
                    LEFT JOIN 
                nomenclature ON order_items.nomenclature_id = nomenclature.id
            WHERE
                nomenclature.category = 'water' AND nomenclature.tare_volume = 'Vol19L'
            GROUP BY order_items.order_id
        ) AS order_sum ON order_sum.order_id = orders.id
            LEFT JOIN
        delivery_points ON delivery_point_id = delivery_points.id
            LEFT JOIN
        delivery_schedule ON delivery_schedule.id = orders.delivery_schedule_id
    WHERE
        (orders.delivery_date BETWEEN @start_date AND @end_date)
        AND orders.order_status NOT IN ('Canceled', 'NewOrder', 'WaitForPayment')
        AND (@report_mode = 0 OR orders.order_status NOT IN ('DeliveryCanceled', 'NotDelivered'))
        AND !orders.self_delivery
        AND !orders.is_contract_closer
        AND !orders.service
        AND delivery_schedule.id IS NOT NULL
        AND orders.delivery_point_id IS NOT NULL) as T
GROUP BY week_begin
ORDER BY week_begin</CommandText>
        <QueryParameters>
          <QueryParameter Name="start_date">
            <Value>=Parameters!start_date</Value>
          </QueryParameter>
          <QueryParameter Name="end_date">
            <Value>=Parameters!end_date</Value>
          </QueryParameter>
          <QueryParameter Name="report_mode">
            <Value>=Parameters!report_mode</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
