    <DataSet Name="Undeliveries">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>#SET @start_date = '2020-08-17';
#SET @end_date = '2020-08-17 23:59';

SELECT
    count(*) as undeliveries
FROM
    undelivered_orders undeliv_orders
        left join
    guilty_in_undelivered_orders guilty_in on undeliv_orders.id=guilty_in.undelivery_id
        left join
    orders old_order on undeliv_orders.old_order_id=old_order.id
WHERE
    old_order.delivery_date &gt;= @start_date
    and old_order.delivery_date &lt;= @end_date
    and (guilty_in.guilty_side = 'Driver' OR guilty_in.guilty_department_id in (13, 26));</CommandText>
        <QueryParameters>
          <QueryParameter Name="start_date">
            <Value>=Parameters!start_date</Value>
          </QueryParameter>
          <QueryParameter Name="end_date">
            <Value>=Parameters!end_date</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="undeliveries">
          <DataField>undeliveries</DataField>
          <TypeName>System.Int64</TypeName>
        </Field>
      </Fields>
