  <DataSets>
    <DataSet Name="Data">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT
    o.id AS order_id,
    o.delivery_date AS delivery_date,
    c.name AS client_name,
    CASE c.person_type
        WHEN 'natural' THEN 'ФЛ'
        WHEN 'legal' THEN 'ЮЛ'
        else c.person_type
    END AS person_type,    
    dp.compiled_address_short AS address_of_delivery,
    GROUP_CONCAT(DISTINCT nomenclature.name SEPARATOR ', ') AS nomenclatures,
    o.sum_to_receive ,
    CASE o.payment_type 
        WHEN 'cash' THEN 'НАЛ'
        WHEN 'cashless' THEN 'БЕЗНАЛ'
        WHEN 'ByCard' THEN 'ПО КАРТЕ'
        WHEN 'Terminal' THEN 'ТЕР-МИНАЛ'
        ELSE o.payment_type
    END AS payment_type,
    gg.name as geographic_group,
    IFNULL(
        GROUP_CONCAT(
            DISTINCT
            CONCAT(dp_ph.number, ' ', IFNULL(dp_ph.name,''))
            SEPARATOR '\n'
        ),
        GROUP_CONCAT(
            DISTINCT
            CONCAT(c_ph.number, ' ', IFNULL(c_ph.name,''))
            SEPARATOR '\n'
        )
    ) AS phone,
    #GROUP_CONCAT(DISTINCT d.name SEPARATOR ', ') AS discount,
    (
        SELECT GROUP_CONCAT(DISTINCT d.name SEPARATOR ', ')
        FROM order_items i
        JOIN discount_reasons d ON i.discount_reason_id = d.id
        WHERE i.order_id = o.id
    ) AS discount,
    cc.name as came_from ,
    o.comment AS comment ,
    GET_PERSON_NAME_WITH_INITIALS(e.last_name, e.name, e.patronymic) AS order_creator,
    sector_versions.sector_name as delivery_district
FROM 
    orders o
    INNER JOIN order_items i ON o.id = i.order_id
    INNER JOIN nomenclature ON nomenclature.id = i.nomenclature_id
    INNER JOIN discount_reasons d ON i.discount_reason_id = d.id
    INNER JOIN counterparty c ON o.id = c.first_order_id
    INNER JOIN delivery_points dp ON dp.id = o.delivery_point_id
    LEFT JOIN employees e ON e.id = o.author_employee_id
    LEFT JOIN counterparty_camefrom cc ON cc.id = c.counterparty_camefrom_id
    INNER JOIN delivery_point_geodata_version geodata ON geodata.delivery_point_id = dp.id
        AND geodata.start_date &gt;= @start_date
        AND (geodata.end_date IS NULL OR geodata.end_date &lt;= @end_date)
    INNER JOIN sectors ON sectors.id = geodata.sector_id
    INNER JOIN sector_versions ON sector_versions.sector_id = sectors.id
         AND sector_versions.start_date &gt;= @start_date
         AND (sector_versions.end_date IS NULL OR sector_versions.end_date &lt;= @end_date)
    LEFT JOIN geographic_groups gg ON gg.id = sector_versions.geographic_group_id
    LEFT JOIN phones dp_ph ON dp_ph.delivery_point_id = dp.id
    LEFT JOIN phones c_ph ON c_ph.counterparty_id = c.id
WHERE 
    o.delivery_date BETWEEN @start_date AND @end_date
    AND (@discount_id = 0 OR d.id = @discount_id)
    AND (@district_id = '' OR sectors.id = @district_id)
GROUP BY o.id DESC
;</CommandText>
