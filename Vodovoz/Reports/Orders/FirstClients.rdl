    <DataSet Name="Stats">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT 
    GROUP_CONCAT(concat_ws(': ' ,stats.came_from ,stats.orders_count ) SEPARATOR ' | ') as stats
FROM    
(
    SELECT
        COUNT(DISTINCT i.order_id)  as orders_count,
        cc.name as came_from
    FROM 
        orders o
        INNER JOIN order_items i ON o.id = i.order_id
        INNER JOIN nomenclature ON nomenclature.id = i.nomenclature_id
        INNER JOIN discount_reasons d ON i.discount_reason_id = d.id
        INNER JOIN counterparty c ON o.id = c.first_order_id
        INNER JOIN delivery_points dp ON dp.id = o.delivery_point_id
        INNER JOIN counterparty_camefrom cc ON cc.id = c.counterparty_camefrom_id
        LEFT JOIN employees e ON e.id = o.author_employee_id
        INNER JOIN delivery_point_geodata_version geodata ON geodata.delivery_point_id = dp.id
            AND geodata.start_date &gt;= @start_date
            AND (geodata.end_date IS NULL OR geodata.end_date &lt;= @end_date)
        INNER JOIN sectors ON sectors.id = geodata.sector_id
        INNER JOIN sector_versions ON sector_versions.sector_id = sectors.id
            AND sector_versions.start_date &gt;= @start_date
            AND (sector_versions.end_date IS NULL OR sector_versions.end_date &lt;= @end_date)
        LEFT JOIN geographic_groups gg ON gg.id = sector_versions.geographic_group_id
        LEFT JOIN phones dp_ph ON dp_ph.delivery_point_id = dp.id
        LEFT JOIN phones c_ph ON c_ph.counterparty_id = c.id
    WHERE 
        o.delivery_date BETWEEN @start_date AND @end_date
        AND (@discount_id = 0 OR d.id = @discount_id)
        AND (@district_id = '' OR sectors.id = @district_id)
    GROUP BY cc.id DESC 
) as stats
;</CommandText>
