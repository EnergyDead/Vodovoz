{
  "ConnectionStrings": {
    "DefaultConnection": "server=<Database Server>;port=3306;database=<Database>;uid=<user>;password=<password>"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Security": {
    "Tokens": {
      "Issuer": "<Issuer>",
      "Audience": "<Audience>",
      "Key": "<SecretKey>",
      "Lifetime": <Token Lifetime In Minutes>
    }
  },
  "Nhibernate": {
    "Server": <Database Server>,
    "Port": 3306,
    "Database": <Database>,
    "UserID": <user>,
    "Password": <password>
  },
  "AllowedHosts": "*"
}
