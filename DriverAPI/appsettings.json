{
  "ConnectionStrings": {
    "DefaultConnection": "server=localhost;port=3306;database=<Database>;uid=<user>;password=<password>"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Security": {
    "Tokens": {
      "Issuer": "<Issuer>",
      "Audience": "<Audience>",
      "Key": "<SecretKey>",
      "Lifetime": <Token Lifetime In Minutes>
    }
  },
  "AllowedHosts": "*"
}
